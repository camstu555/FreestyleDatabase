<div class="statBlock">
    <div class="number">
        <h2>@Wins - @Losses</h2>
        <h3>W - L</h3>
        <h5>(since @Since.Value.Date.ToShortDateString())</h5>
    </div>
    <div class="number">
        <h2>@Pins</h2>
        <h3>Pins</h3>
    </div>
    <div class="number">
        <h2>@Techs</h2>
        <h3>Tech</h3>
    </div>
    <div class="number">
        <h2>@Points</h2>
        <h3>Decisions</h3>
    </div>
    <div class="number">
        <h2>@Bonus.ToString("P")</h2>
        <h3>Bonus Rate</h3>
    </div>
    <div class="number">
        <h2>@OffPts</h2>
        <h3>Average Points Scored</h3>
    </div>
    <div class="number">
        <h2>@DefPts</h2>
        <h3>Average Points Given Up</h3>
    </div>
</div>